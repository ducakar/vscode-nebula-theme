{
    "name": "nebula-theme",
    "displayName": "Nebula Theme",
    "description": "ðŸŒŒðŸŒ¸ Fresh colors on a dark, cold slate. An almost-material theme for Visual Studio Code.",
    "version": "1.0.5",
    "publisher": "ChirtleLovesDolls",
    "engines": {
        "vscode": "^1.21.0"
    },
    "license": "mit",
    "categories": [
        "Themes"
    ],
    "icon": "images/icon.png",
    "galleryBanner": {
        "color": "#27273A",
        "theme": "dark"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "contributes": {
        "themes": [
            {
                "label": "Nebula",
                "uiTheme": "vs-dark",
                "path": "./themes/Nebula-color-theme.json"
            },
            {
                "label": "Nebula - Console",
                "uiTheme": "vs-dark",
                "path": "./themes/Nebula-color-theme-console.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Nebula Theme Configuration",
            "properties": {
                "nebula-theme.commentItalics": {
                    "type": "boolean",
                    "defualt": true
                },
                "nebula-theme.themeItalics": {
                    "type": "string",
                    "default": "basic",
                    "enum": [
                        "none",
                        "basic",
                        "more",
                        "operator"
                    ]
                },
                "nebula-theme.materialize": {
                    "type": "boolean",
                    "defualt": false
                }
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eating-coleslaw/vscode-nebula-theme"
    },
    "bugs": {
        "url": "https://github.com/eating-coleslaw/vscode-nebula-theme/issues"
    }
}
