  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#919cb9",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "meta.brace",
        "punctuation"
      ],
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "String",
      "scope": [
        "markup.quote",
        "punctuation.definition.string",
        "string"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Escape Character, Regular Expression",
      "scope": [
        "constant.character.escape",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "entity.name.section",
        "keyword",
        "storage"
      ],
      "settings": {
        // "foreground": "#E752A1"
        "foreground": "#ff6088"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage.type"
      ],
      "settings": {
        // "foreground": "#F36F98"
        "foreground": "#c062ff"
      }
    },
    {
      "name": "Namespace, Preprocessor, Annotation",
      "scope": [
        "entity.name.namespace",
        // "meta.preprocessor",
        "meta.type.annotation"
      ],
      "settings": {
        // "foreground": "#FAFAA0"
        "foreground": "#f7f782"
      }
    },
    {
      "name": "Selector, Tag",
      "scope": [
        "entity.name.selector",
        "entity.name.tag",
        "entity.other",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#99DDFF"
      }
    },
    {
      "name": "Type",
      "scope": [
        "meta.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#50DFFE"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#97EE91"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#E7ADFB"
      }
    },
    {
      "name": "URL",
      "scope": [
        "*link*",
        "*uri*",
        "*url*"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
      }
    },
    {
      "name": "Markup Bold Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold"
      ],
      "settings": {
        "fontStyle": "italic bold",
      }
    },
    {
      "name": "Markup Underline",
      "scope": [
        "markup.underline",
      ],
      "settings": {
        "fontStyle": "underline",
      }
    },
    {
      "name": "Inserted",
      "scope": [
        "markup.inserted"
      ],
      "settings": {
        "foreground": "#97EE91"
      }
    },
    {
      "name": "Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#E34F8C"
      }
    },
    {
      "name": "Changed",
      "scope": [
        "markup.changed"
      ],
      "settings": {
        "foreground": "#FAFAA0"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#6796E6"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#CD9731"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#F44747"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#B267E6"
      }
    }
  ]
}
